

# This is the config file for VASimRelease.
# It is encourged to use ${VASim_Dir} in following variables to replace the VASimRelease folder

#####################################################
# Output folders and files                          #
#####################################################

# Where is the output folder?
# (the output folder will be automatically created)
# example: VASim_Output_Dir="/local/mnt/workspace/VASimRelease/output"
VASim_Output_Dir="${VASim_Dir}/output"

#####################################################
# Inputs folders and files                          #
#####################################################

# Where is the VASim config file?
# example: VASim_Config="/local/mnt/workspace/VASimRelease/input/VASim_config.txt"
VASim_Config="${VASim_Dir}/sample_input/VASim_config.txt"

# The input file list for VASimAutoTest
# example: VASim_File_Input="${VASim_Dir}/sample_input/VASim_file_input_template.csv"
VASim_File_Input="${VASim_Dir}/sample_input/VASim_file_input_template.csv"

# The video input folder
# example: VASim_Video_Input_Folder="${VASim_Dir}/sample_input"
VASim_Video_Input_Folder="/usr/data/VAM/sample_input"

# The rule configuration input folder
# example: VASim_VAM_Rule_Input_Folder="${VASim_Dir}/sample_input"
VASim_VAM_Rule_Input_Folder="${VASim_Dir}/sample_input"

# The ground truth input folder
# example: VASim_Ground_Truth_Input_Folder="${VASim_Dir}/sample_input"
VASim_Ground_Truth_Input_Folder="${VASim_Dir}/sample_input"

# The FR enrollment input folder
# example: VASim_FR_Enrollment_File="${VASim_Dir}/sample_input/VASim_FR_enrollment_template.csv"
VASim_FR_Enrollment_File="${VASim_Dir}/sample_input/VASim_FR_enrollment_template.csv"

# The FR enrollment input folder
# example: VASim_FR_Enrollment_Folder="${VASim_Dir}/sample_input"
VASim_FR_Enrollment_Folder="${VASim_Dir}/FR_input"

#####################################################
# VASim Functions                                   #
#####################################################
# Dry run mode (enable/disable) only only print all command but not execute it
Enable_DryRun_Only="disable"

# Enable VASim process (enable/disable)
Enable_VASim="enable"

# Enable report (enable/disable)
Enable_VAMReport="enable"

# Enable renderer (enable/disable)
Enable_Renderer="disable"

# Enable face recognition mode (enable/disable)
Enable_FR="disable"

# Enable snapshot for all events (enable/disable)
Enable_Snapshot="disable"

# Enable loop for demo or logevity test, it will loops 1000 times (enable/disable)
Enable_Loop="disable"

# Enable filmstrip generator for n times (per 60 frames)
FilmStripNum="0"

