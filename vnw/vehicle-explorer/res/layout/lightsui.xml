            android:layout_height="60px"
            android:text="@string/AutomaticHeadLights"/>

            <TextView
            android:layout_width="240px"
            android:layout_height="60px"
            android:text="@string/DynamicHighBeam"/>

            <TextView
            android:layout_width="240px"
            android:layout_height="60px"
            android:text="@string/HighBeam"/>

            <TextView
            android:layout_width="240px"
            android:layout_height="60px"
            android:text="@string/LeftIndicator"/>

            <TextView
            android:layout_width="240px"
            android:layout_height="60px"
            android:text="@string/RightIndicator"/>

            <TextView
            android:layout_width="240px"
            android:layout_height="60px"
            android:text="@string/BrakeLight"/>

            <TextView
            android:layout_width="240px"
            android:layout_height="60px"
            android:text="@string/FogFrontLight"/>
            <TextView
            android:layout_width="240px"
            android:layout_height="60px"
            android:text="@string/FogRearLight"/>

            <TextView
            android:layout_width="240px"
            android:layout_height="60px"
