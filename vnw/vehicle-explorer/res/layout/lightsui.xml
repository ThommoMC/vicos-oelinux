            android:layout_width="240px"
            android:layout_height="60px"
            android:text="@string/IntDriverLight"/>

            <TextView
            android:layout_width="240px"
            android:layout_height="60px"
            android:text="@string/IntPsngrLight"/>

            <TextView
            android:layout_width="240px"
            android:layout_height="60px"
            android:text="@string/IntCenterLight"/>

            <TextView
            android:layout_width="240px"
            android:layout_height="60px"
            android:text="@string/Int3rdRowLight"/>

            <Button
            android:id="@+id/HomeUIbackBtn"
            android:layout_width="240px"
            android:layout_height="60px"
            android:text="@string/UIBackBtn" />
        </GridLayout>
        <GridLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
